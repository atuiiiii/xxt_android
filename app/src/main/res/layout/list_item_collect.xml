<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data>
        <variable
            name="collect"
            type="bid.xiaocha.xxt.model.CollectResult"/>
    </data>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <TextView
        android:id="@+id/tv_serve_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text='@{@string/title+collect.offerServeEntity.title}'
        />
    <TextView
        android:id="@+id/tv_serve_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text='@{@string/title+collect.offerServeEntity.content}'
        />
    <TextView
        android:id="@+id/tv_collect_date"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text='@{@string/title+collect.date}'
        />

</LinearLayout>
</layout>